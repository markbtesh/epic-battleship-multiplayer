{
  "builds": [
    {
      "src": "battleship-multiplayer/angular.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/battleship-multiplayer" // Ensure this matches the `outputPath`
      }
    }
  ],
  "rewrites": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
